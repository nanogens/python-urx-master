            # 1.0 CCW      , -1.0  lift     ,-1.0 lift,   -1.0 lift  , -1.0 rotation  , -1.0 rotation
            rob.movej((6.3, -3.15, 2.1, 0, 0, 0), acc=a, vel=v)

            print("stop robot")
            rob.stopj()
            time.sleep(1)

            pose = rob.getl()
            print("robot tcp is at: ", pose)


